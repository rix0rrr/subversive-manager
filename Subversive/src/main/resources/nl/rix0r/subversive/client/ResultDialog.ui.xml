<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <g:HTMLPanel>
        <h2>Save Changes</h2>
        <div class="explanation"></div>

        <div class="screenControls">
            <g:Label ui:field="summary" />

            <div class="applicationErrors">
                <g:VerticalPanel ui:field="messages" />
            </div>

            <div class="buttons">
            </div> <g:Button ui:field="okButton">OK</g:Button>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>